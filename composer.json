{
  "name": "northrook/latte-bundle",
  "type": "library",
  "description": "Nette\\Latte integration for Northrook packages.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\": "src"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "latte/latte": "@stable",
    "northrook/core": "dev-main",
    "northrook/filesystem": "dev-main",
    "northrook/minify": "dev-main",
    "northrook/html-element": "dev-main",
    "northrook/html-formatter": "dev-main",
    "northrook/latte-symfony-cache": "dev-main"
  },
  "require-dev": {
    "phpstan/phpstan": " @stable",
    "northrook/dev-env": "dev-main",
    "symfony/cache": "^7.1"
  }
}